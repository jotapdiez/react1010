{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "space-infix-ops": 1,
    "key-spacing": 1,
    "no-mixed-spaces-and-tabs": 1,
    "comma-dangle": 1,
    "semi-spacing": 1,
	"eqeqeq": 1
	
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}
